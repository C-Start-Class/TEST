#include <stdio.h>

int main()
{
    int n;
    int count = 0;
    printf("Nhap gia tri n: ");
    scanf("%d",&n);
    int max = 1;
    int min = n;
    for(int i = 2; i < n; i++){
        if(n%i==0){
            count++;
            if(i>max){
                max = i;
            }
            if(i<min){
                min = i;
            }
        }
    }
    if(count==0){
        printf("%d la so nguyen to",n);
    }
    else{
        printf("%d co %d uoc so\n",n,count);
        printf("uoc so lon nhat la %d\n",max);
        printf("uoc so nho nhat la %d\n",min);
    }
}
