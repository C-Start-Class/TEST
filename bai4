#include <stdio.h>

int main()
{
    int n;
    int count = 0;
    printf("Nhap gia tri n: ");
    scanf("%d",&n);
    int max = 1;
    int min = n;
    for(int i = 1; i <= n; i++){
        if(n%i==0){
            count++;
            if(i>max&&i!=n){
                max = i;
            }
            if(i<min&&i!=1){
                min = i;
            }
        }
    }
        printf("%d co %d uoc so\n",n,count);
        printf("uoc so lon nhat cua %d la %d\n",n,max);
        printf("uoc so nho nhat cua %d la %d\n",n,min);
}
