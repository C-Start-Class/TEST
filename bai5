#include <stdio.h>

int main()
{
    int n;
    int sum = 0;
    printf("Nhap gia tri n: ");
    scanf("%d",&n);
    for(int i = 2; i < n; i++){
        if(n%i==0){
            if(i%2==0){
                sum+=i;
            }
    }
    }
    if(sum==0){
        printf("%d la so nguyen to",n);
    }
    else{
        printf("Tong cac uoc so chan la %d",sum);
    }
}
